#include <iostream>
int poten(int x, int y);
using namespace std;

int main()
{ int x,y;
    cout<<"Ingrese el valor para x: "<<endl;
    cin>>x;
    cout<<"Ingrese el valor para y: "<<endl;
    cin>>y;
    cout<<"La potencia de"<<poten(x,y)<<endl;

    return 0;
}

int poten(int x, int y){
    int acum=1;
    for (int i=1; i<=y;i++)
    acum=acum*x;
    return acum;
}
